!!! 5
html
    head
        title Android Forensic Timeline
        link(rel='stylesheet', href='/css/bootstrap.min.css')
        link(rel='stylesheet', href='/css/bootstrap-responsive.min.css')
        meta(name='viewport', 'content'='user-scalable=yes, width=device-width, initial-scale=0.8, maximum-scale=1.2')
        link(rel='stylesheet', href='/css/style.css')
        link(rel='stylesheet', href='/css/opentip.css')
        -each script in globalScripts
            script(type = 'text/javascript', src = script)

    body
        div.progress.progress-striped.active#progress-bar
            div.bar#progress-indicator
        div#event-detail-pane.popup-div
            div#event-display-pane
                p.nav-header.text-center Event Details
                div.container-fluid
                    div#event-metadata.span4
                        ul.nav.nav-pills.nav-stacked
                            li.nav-header Date
                            li
                                div#date-display
                            li.nav-header.cozy-list-header Process ID
                            li
                                div#pid-display
                            li.nav-header.cozy-list-header System Objects
                            li
                                div#object-display
                                    table#object-table.table.table-condensed.table-hover
                                        tbody#object-tbody
                    div#event-message.span5
                        p.nav-header Messages
                        textarea(rows = "11", readonly)#message-display
                    div.span3
                        ul.nav.nav-pills.nav-stacked
                            li.nav-header Relevance Query
                            li
                                div.input-append
                                    input(type = "text", placeholder = "application name").span3#relevance-selection-input
                                    button(type = "button").btn.btn-warning#relevance-search Feel lucky
                            li.nav-header.cozy-list-header App Trace Query
                            li
                                div.input-append
                                    input(type = "text", placeholder = "application name").span3#app-trace-selection-input
                                    button(type = "button").btn.btn-success#app-trace-search Trace
                            li.nav-header.cozy-list-header App File Activity
                            li
                                div.input-append
                                    input(type = "text", placeholder = "application name").span3#file-activity-selection-input
                                    button(type = "button").btn.btn-info#file-activity-search Search
                            li.nav-header.cozy-list-header Dmesg Query
                            li
                                div.input-append
                                    input(type = "text", placeholder = "keywords ...").span3#dmesg-selection-input
                                    button(type = "button").btn#dmesg-search Query
            div.popup-ctrl-bar
                div(title="Expand event pane").popup-ctrl
        div#aggregation-pane.dropdown-div
            div#aggregation-arena
            div.dropdown-ctrl-bar
                div(title="Expand aggregation pane").dropdown-ctrl
        div#dropdown-ctrl-pane.popup-div
            div#extra-ctrl-pane
                p.nav-header Timeline Brush
                #time-brush
        div#responsive-pane.slide-right-div
            div.slide-right-ctrl-bar
                div(title="Expand responsive pane").slide-right-ctrl
            div#responsive-app-pane
                p.nav-header Detected Applications
                #responsive-apps
                p.nav-header Device Booted [UTC]
                #temporal-info
                p.nav-header Timezone Setting
                #timezone-info
                p.nav-header Running Time
                #running-time
            //div.responsive-div.tabbable
            //    ul.nav.nav-tabs
            //        li.active
            //            a(href = "#main-tab", data-toggle = "tab") Main
            //        li
            //            a(href = "#extend-tab", data-toggle = "tab") Extend
            //    div.tab-content
            //        div#main-tab.tab-pane.active
            //            p.nav-header Responsive IDs
            //            div#responsive-ids
            //            p.nav-header Responsive Objects
            //            div#responsive-objects
            //        div#extend-tab.tab-pane
            //            p.nav-header Responsive IDs
            //            div#responsive-ids-extend
        div#main-ctrl-pane.slide-left-div
            div.slide-left-ctrl-bar
                div(title="Expand control pane").slide-left-ctrl
            div.ctrl-panel
                ul.nav.nav-pills.nav-stacked
                    li.nav-header Extent Query Target
                    li
                        select#collection-input
                            option(value='android_logs:main') main
                            option(value='android_logs:system') system
                            option(value='android_logs:events') events
                            option(value='android_logs:radio') radio
                    li.nav-header.cozy-list-header Content Specification
                    li
                        input(type="text", placeholder="keywords ...")#selection-input
                    li.nav-header.cozy-list-header Timeline Window
                    li
                        select#time-window-start.selection-field
                            option from...
                        select#time-window-end.selection-field
                            option to...
                    li.nav-header.cozy-list-header Objects Selection
                    li
                        select(multiple="multiple")#objects.selection-field
                    li.nav-header.cozy-list-header IDs Selection
                    li
                        select(multiple="multiple")#ids.selection-field
                    li.nav-header.cozy-list-header Aggregation Type
                    li
                        select#map-reduce-type.selection-field
                            option aggregate by ...
                        //button#aggregate-btn.btn.btn-success.btn-small Aggregate
                    li
                        div.btn-group
                            //button#search.btn.btn-success.btn-small Search
                            button#aggregate-btn.btn.btn-success.btn-small Aggr.
                            button#expand.btn.btn-warning.btn-small Expand
                            button#filter.btn.btn-info.btn-small Filter
                            button#clear.btn.btn-danger.btn-small Clear
                    //li.nav-header Time Brush [Seconds]
                    //li
                    //    #time-brush
        div#content
            p.nav-header Android Timeline Analysis
            #timeline_main.timeline
        a#zoom-out
        a#zoom-in
        a#reset-scale
        a#trash
        script(type = 'text/javascript', src = '/js/operator.js')






